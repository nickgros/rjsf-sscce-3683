  "scripts": {
    "build:production": ". ./env/production.sh;export REACT_APP_CONFIG=`cat ./env/template.json| jq '' -c | envsubst`;NODE_ENV=production webpack --mode production",
    "build:staging": ". ./env/staging.sh;export REACT_APP_CONFIG=`cat ./env/template.json| jq '' -c | envsubst`;NODE_ENV=production webpack --mode production",
    "start": ". ./env/${SOURCE_ENV_FILE_NAME:-'localhost'}.sh;REACT_APP_CONFIG=`cat ./env/template.json| jq '' -c | envsubst` NODE_ENV=development webpack serve",
    "start_windows": "webpack serve",
    "start:prod": ". ./env/production.sh;REACT_APP_CONFIG=`cat ./env/template.json| jq '' -c | envsubst` NODE_ENV=production webpack serve",
    "start:local": ". ./env/localhost.sh;REACT_APP_CONFIG=`cat ./env/template.json| jq '' -c | envsubst` NODE_ENV=development webpack serve",
    "build:local": ". ./env/localhost.sh;export REACT_APP_CONFIG=`cat ./env/template.json| jq '' -c | envsubst`;NODE_ENV=production webpack --mode production",
    "fresh:dev": "yarn clean:all && yarn && yarn start",
    "fresh:prod": "yarn clean:all && yarn && yarn start:prod",
    "clean:all": "rm -rf dist node_modules yarn.lock package-lock.json",
    "test": ". ./env/localhost.sh;export REACT_APP_CONFIG=`cat ./env/template.json | jq '' -c | envsubst`;NODE_ENV=production jest",
    "test:watch": "jest --watch",
    "test:detect": "jest --detectOpenHandles",
    "errors:prod": "NODE_ENV=production webpack -w",
    "errors:dev": "NODE_ENV=development webpack -w",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "@date-io/core": "1.3.6",
    "@date-io/date-fns": "1.3.13",
